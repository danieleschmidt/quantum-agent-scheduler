{
  "name": "project-name",
  "version": "1.0.0",
  "description": "Development tooling for the project",
  "scripts": {
    "docs:serve": "python -m http.server 8000 --directory docs",
    "docs:build": "mkdocs build",
    "docs:deploy": "mkdocs gh-deploy",
    "lint": "poetry run ruff check . && poetry run mypy src/",
    "lint:fix": "poetry run ruff check --fix . && poetry run black src/ tests/",
    "test": "poetry run pytest tests/ -v",
    "test:unit": "poetry run pytest tests/unit/ -v",
    "test:integration": "poetry run pytest tests/integration/ -v",
    "test:quantum": "poetry run pytest tests/quantum/ -v --quantum",
    "test:coverage": "poetry run pytest --cov=src/quantum_scheduler --cov-report=html --cov-report=term",
    "format": "poetry run black src/ tests/ && poetry run isort src/ tests/",
    "format:check": "poetry run black --check src/ tests/ && poetry run isort --check-only src/ tests/",
    "typecheck": "poetry run mypy src/",
    "build": "poetry build",
    "dev": "poetry run uvicorn src.quantum_scheduler.api.main:app --reload --host 0.0.0.0 --port 8080",
    "clean": "rm -rf build/ dist/ .pytest_cache/ .coverage htmlcov/ .mypy_cache/ .ruff_cache/",
    "security-check": "poetry run safety check && poetry run bandit -r src/",
    "pre-commit": "poetry run pre-commit run --all-files",
    "quantum-test": "poetry run python scripts/validate_quantum_configs.py",
    "benchmark": "poetry run python scripts/compare_benchmarks.py",
    "compatibility": "poetry run python scripts/check_compatibility.py"
  },
  "devDependencies": {},
  "private": true
}